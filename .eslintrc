{
    "parser": "babel-eslint",
    "extends": [
        "prettier",
        "keystone"
    ],
    "plugins": [
        "prettier",
        "import"
    ],
    "rules": {
        "indent": 0,
        "no-tabs": 0,
        "no-underscore-dangle": 0,
        "quote-props": 0,
        "space-before-function-paren": 0,
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "trailingComma": "all",
            "bracketSpacing": true,
            "jsxBracketSameLine": true,
            "parser": "flow"
        }]
    }
}
