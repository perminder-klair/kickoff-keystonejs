{
    "extends": [
        "prettier",
        "keystone"
    ],
    "plugins": [
        "prettier",
        "import"
    ],
    "rules": {
        "prettier/prettier": ["error", {
            "singleQuote": true,
            "trailingComma": "all",
            "bracketSpacing": true,
            "jsxBracketSameLine": true,
            "parser": "flow",
            "useTabs": true,
            "tabWidth": 4
        }],
        "indent": 0,
        "no-tabs": 0,
        "no-underscore-dangle": 0,
        "quote-props": 0,
        "space-before-function-paren": 0
    }
}
