{
    "extends": "keystone",
    "rules": {
        "strict": 0,
        "indent": [2, 4],
        "comma-dangle": 0,
        "new-cap": 0,
        "camelcase": 0,
        "class-methods-use-this": 0,
        "max-len": ['error', 280, 4],
        "no-param-reassign": ['error', { 'props': false }],
        "no-underscore-dangle": 0
    }
}
